            <div class="form-outline form-white mb-4">
              <input type="email" id="form5Example29" class="form-control" style="border: 2px solid #FFFFFF;" placeholder="Email" />
              <label class="form-label" for="form5Example29"></label>
            </div>
          </div>
          <!--Grid column-->

          <!--Grid column-->
          <div class="col-auto">
